{
  "name": "@devtree/react-pack",
  "version": "0.0.8",
  "description": "Simple tools and ESLint, Prettier configuration for React, Next.js development",
  "keywords": [
    "dev",
    "development",
    "devtree",
    "eslint",
    "next",
    "prettier",
    "react",
    "typescript"
  ],
  "files": [
    "eslint",
    "prettier.js"
  ],
  "scripts": {
    "git:push": "git add . && git commit -m 'fix' && git push",
    "publish:patch": "npm version patch && npm publish",
    "publish:alpha": "npm version prerelease --preid alpha && npm publish",
    "v:patch": "npm version patch",
    "v:alpha": "npm version prerelease --preid alpha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tetritz/devtree.git",
    "directory": "packages/react-pack"
  },
  "bugs": {
    "url": "https://github.com/tetritz/devtree/issues"
  },
  "author": "Lucas Choi <eterv7@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "eslintConfig": {
    "extends": [
      "./eslint/base"
    ]
  },
  "prettier": "./prettier"
}
