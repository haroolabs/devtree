{
  "name": "@devtree/react-pack",
  "version": "0.0.7",
  "description": "Simple tools and ESLint, Prettier configuration for React, Next.js development",
  "files": [
    "compiled",
    "eslint",
    "prettier.js"
  ],
  "scripts": {
    "git:push": "git add . && git commit -m 'init' && git push",
    "publish:patch": "npm version patch && npm publish",
